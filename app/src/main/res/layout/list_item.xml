<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context="com.dvb.mybody.CatalogActivity">


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_alignWithParentIfMissing="false"
        android:layout_marginBottom="20dp"
        android:layout_marginTop="20dp"
        android:adjustViewBounds="false"
        android:longClickable="false"
        app:srcCompat="@mipmap/body_lines_center"/>


    <RelativeLayout
        android:id="@+id/relativeLayout2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true">


        <TextView
            android:id="@+id/day_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="10dp"
            android:text="22 September 2017"
            android:textSize="18sp"/>


        <TextView
            android:id="@+id/measureH"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/day_num"
            android:layout_below="@+id/day_num"
            android:layout_marginTop="50dp"
            android:ems="4"
            android:text="HEIGHT"
            android:textSize="14sp"/>

        <TextView
            android:id="@+id/height_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/measureH"
            android:layout_below="@+id/measureH"
            android:textSize="18sp"
            tools:text="191"/>

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/height_num"
            android:layout_alignBottom="@+id/height_num"
            android:layout_toEndOf="@+id/textView20"
            android:text=" cm"
            android:textSize="14sp"/>

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/height_num"
            android:layout_below="@+id/height_num"
            android:layout_marginTop="10dp"
            android:text="SHOULDERS"
            android:textSize="14sp"/>

        <TextView
            android:id="@+id/shoulders_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/textView14"
            android:layout_below="@+id/textView14"
            android:text="120"
            android:textSize="18sp"/>

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/shoulders_num"
            android:layout_alignBottom="@+id/shoulders_num"
            android:layout_toEndOf="@+id/shoulders_num"
            android:text=" cm"
            android:textSize="14sp"/>

        <TextView
            android:id="@+id/textView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/shoulders_num"
            android:layout_below="@+id/shoulders_num"
            android:layout_marginTop="10dp"
            android:text="CHEST"
            android:textSize="14sp"/>

        <TextView
            android:id="@+id/chest_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/textView17"
            android:layout_below="@+id/textView17"
            android:text="100"
            android:textSize="18sp"/>

        <TextView
            android:id="@+id/textView19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/chest_num"
            android:layout_alignBottom="@+id/chest_num"
            android:layout_toEndOf="@+id/chest_num"
            android:text=" cm"
            android:textSize="14sp"/>

        <TextView
            android:id="@+id/textView20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/chest_num"
            android:layout_below="@+id/chest_num"
            android:layout_marginTop="10dp"
            android:text="ARM"
            android:textSize="14sp"/>

        <TextView
            android:id="@+id/arms_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/textView20"
            android:layout_below="@+id/textView20"
            android:text="35"
            android:textSize="18sp"/>

        <TextView
            android:id="@+id/textView22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/arms_num"
            android:layout_alignBottom="@+id/arms_num"
            android:layout_toEndOf="@+id/arms_num"
            android:text=" cm"
            android:textSize="14sp"/>

        <TextView
            android:id="@+id/textView23"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/arms_num"
            android:layout_below="@+id/arms_num"
            android:layout_marginTop="10dp"
            android:text="BELLY"
            android:textSize="14sp"/>

        <TextView
            android:id="@+id/belly_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/textView23"
            android:layout_below="@+id/textView23"
            android:text="92"
            android:textSize="18sp"/>

        <TextView
            android:id="@+id/textView25"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/belly_num"
            android:layout_alignBottom="@+id/belly_num"
            android:layout_toEndOf="@+id/belly_num"
            android:text=" cm"
            android:textSize="14sp"/>

        <TextView
            android:id="@+id/textView26"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/belly_num"
            android:layout_below="@+id/belly_num"
            android:layout_marginTop="10dp"
            android:text="HIP"
            android:textSize="14sp"/>

        <TextView
            android:id="@+id/hip_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/textView26"
            android:layout_below="@+id/textView26"
            android:text="54"
            android:textSize="18sp"/>

        <TextView
            android:id="@+id/textView28"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/hip_num"
            android:layout_alignBottom="@+id/hip_num"
            android:layout_toEndOf="@+id/hip_num"
            android:text=" cm"
            android:textSize="14sp"/>

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/weight_num"
            android:layout_alignBottom="@+id/weight_num"
            android:layout_toEndOf="@+id/weight_num"
            android:text=" kg"/>

        <TextView
            android:id="@+id/weightH"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/hip_num"
            android:layout_below="@+id/hip_num"
            android:layout_marginTop="50dp"
            android:hint="WEIGHT"
            android:text="WEIGHT"/>

        <TextView
            android:id="@+id/weight_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/weightH"
            android:layout_below="@+id/weightH"
            android:text="60"
            android:textSize="24sp"/>
    </RelativeLayout>


</RelativeLayout>
